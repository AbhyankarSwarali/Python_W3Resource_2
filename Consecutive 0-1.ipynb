    "def zero_one(s): \n",
    "    while '01' in s:\n",
    "        s = s.replace('01','')\n",
    "    return len(s) == 0\n",
    "s1 = '0101010101'\n",
    "s2 = '0000'\n",
    "s3 = '000111000111'\n",
    "print(zero_one(s1))\n",
    "print(zero_one(s2))\n",
    "print(zero_one(s3))"
   ]
  }
